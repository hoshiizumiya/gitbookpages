{
  "title": "hoshiiumiya的GitBook",
  "description": "这是一个示例书籍",
  "language": "zh-hans",
  "author": "作者名字",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "hide-element",
    "github",
    "expandable-chapters-small",
    "splitter",
    "chapter-fold",
    "search-pro",
    "-sharing",
    "-search",
    "-lunr",
    "insert-logo",
    "code",
    "back-to-top-button",
    "custom-favicon"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/hoshiizumiya"
    },
    "insert-logo": {
      "url": "",
      "style": "background: none; max-height: 50px; min-height: 50px"
    },
    "hide-element": {
      "elements": [ ".gitbook-link" ]
    },
    "favicon": "icon/favicon.ico"
  }
  }